    # DeepL Translate | latvian->spanish
    input="${@:-$(</dev/stdin)}"
    baseurl="https://www.deepl.com/translator"
    query="#lv/es/${input}"

    if ps -e | grep -q GeckoMain; then
        ( { /usr/bin/firefox --new-tab "${baseurl}${query}"; } >/dev/null 2>&1 & )
    else
        ( { /usr/bin/firefox --new-instance "${baseurl}${query}"; } >/dev/null 2>&1 & )
    fi
}

