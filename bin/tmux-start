!#/usr/bin/env bash

if command -v tmux >/dev/null 2>&1; then
        if [[ -z "$TMUX" ]]; then
            ID="$(tmux ls | grep -vm1 attached | cut -d: -f1)"
            if [[ -z "$ID" ]]; then 
                tmux new-session
            else
                tmux attach-session -t "$ID"
            fi
        fi
    fi
}
